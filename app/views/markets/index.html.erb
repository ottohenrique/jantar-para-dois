<div class="row">
  <div class="card">
    <div class="card-content">
      <h3 class="card-title">Mercados próximos a você</h3>

      <ul class="collection">
        <% @markets.each do |market| %>
        <li class="collection-item"><%= link_to market.name, market_path(market) %></li>
        <% end %>
      </ul>
    </div>
  </div>
</div>